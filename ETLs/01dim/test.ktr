    <connection>DES_DB</connection>
    <sql>select 
src_table, des_table , cube_name,
 if(src_table like 'fact%', 'fact',(if(src_table like 'br%', 'br', (if(src_table like '%intersect%', 'dim_intersect', (if(src_table like 'dim%', 'dim', src_table))))))) as color_src,
 if(des_table like 'fact%', 'fact',(if(des_table like 'br%', 'br', (if(des_table like '%intersect%', 'dim_intersect', (if(des_table like 'dim%', 'dim', des_table))))))) as color_des	
 from marlobi.stg_cube_structure_info order by src_table
 	</sql>
    <limit>0</limit>
